{
  "service": "dalr-post-fund-account",
  "plugins": ["serverless-offline"],
  "provider": {
    "name": "aws",
    "runtime": "nodejs14.x",
    "stage": "dev",
    "region": "us-west-2"
  },
  "functions": {
    "postFundAccount": {
      "handler": "src/lambda/postFundAccount.handler",
      "events": [
        {
          "httpApi": "POST /postFundAccount"
        }
      ]
    }
  }
}
